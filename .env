# PM Application v2 Environment Variables

# Server Configuration
NODE_ENV=development
PORT=3001
HOST=localhost

# Database - MySQL
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=rootpassword
DB_NAME=pm_application_v2

# JWT Secrets (generated secure keys)
JWT_SECRET=pm-app-jwt-secret-key-2024-super-secure-32-chars-minimum
JWT_REFRESH_SECRET=pm-app-refresh-secret-key-2024-super-secure-32-chars-minimum

# Cookie Secret (generated secure key)
COOKIE_SECRET=pm-app-cookie-secret-key-2024-super-secure-32-chars-minimum

# CORS
CORS_ORIGIN=http://localhost:3000

# Logging
LOG_LEVEL=info
